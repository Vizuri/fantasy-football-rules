      <type>TeamRoster</type>
      <fieldData>
        <field-data>
          <name>leagueId</name>
          <value>5</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>slotNumber</name>
          <value>1</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>playerId</name>
          <value>4</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>teamId</name>
          <value>1</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$teamRoster</name>
      <isModify>false</isModify>
