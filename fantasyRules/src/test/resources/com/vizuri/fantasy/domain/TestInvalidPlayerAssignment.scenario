  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>LeagueRoster</type>
      <fieldData>
        <field-data>
          <name>leagueId</name>
          <value>5</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>slot</name>
          <value>1</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>validPositions</name>
          <collectionFieldList>
            <field-data>
              <name>validPositions</name>
              <value>&quot;QB&quot;</value>
              <nature>1</nature>
            </field-data>
            <field-data>
              <name>validPositions</name>
              <value>&quot;TE&quot;</value>
              <nature>1</nature>
            </field-data>
          </collectionFieldList>
        </field-data>
      </fieldData>
