          <fieldName>leagueId</fieldName>
          <expected>5</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>teamId</fieldName>
          <expected>1</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>slotNumber</fieldName>
          <expected>1</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>Violation</name>
      <anonymous>true</anonymous>
