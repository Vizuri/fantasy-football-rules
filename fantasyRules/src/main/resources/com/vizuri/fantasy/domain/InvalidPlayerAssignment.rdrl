package com.vizuri.fantasy.domain;

import java.lang.Number;

rule "InvalidPlayerAssignment"
	dialect "mvel"
	when
		LeagueRoster( $leagueId : leagueId, $slotNumber : slot, $positions : validPositionCSVList)
		TeamRoster( leagueId == $leagueId , slotNumber == $slotNumber , player != null )
	then
end
