package com.vizuri.fantasy_football_brms;

import com.vizuri.rules.domain.League;

rule "League Size Rule"
	dialect "mvel"
	when
		league : League( nflTeamsSize > 8 , valid == true )
	then
		league.setValid( false );
		league.setComment( "No more than 8 fantasy teams are allowed in a league" );
end
