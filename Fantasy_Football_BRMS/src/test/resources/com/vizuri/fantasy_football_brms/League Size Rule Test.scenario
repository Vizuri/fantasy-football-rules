<scenario>
  <name>League Size Rule Test.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>League</type>
      <fieldData>
        <field-data>
          <name>nflTeamsSize</name>
          <value>20</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>valid</name>
          <value>true</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>league</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>valid</fieldName>
          <expected>false</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
        <expect-field>
          <fieldName>comment</fieldName>
          <expected>No more than 8 fantasy teams are allowed in a league</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>league</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>com.vizuri.fantasy_football_brms</packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.rules.domain.League</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>