<scenario>
  <name>Fantasy Team Lower Bound.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>FantasyTeam</type>
      <fieldData>
        <field-data>
          <name>name</name>
          <value>teamB</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>id</name>
          <value>1000</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$team</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>Player</type>
      <fieldData>
        <field-data>
          <name>fantasyTeamId</name>
          <value>1000</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$player1</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>severityString</fieldName>
          <expected>ERROR</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>title</fieldName>
          <expected>teamA</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>details</fieldName>
          <expected>Invalid Roster Size</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>Violation</name>
      <anonymous>true</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>true</inclusive>
  <packageName>com.vizuri.fantasy_football_brms</packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.rules.domain.FantasyTeam</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.rules.domain.Player</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.rules.domain.Severity</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.vizuri.rules.domain.Violation</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>